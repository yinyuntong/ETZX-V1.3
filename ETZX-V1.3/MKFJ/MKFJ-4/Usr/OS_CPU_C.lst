 200                  void StkDelA(uint8 TaskID)
 201          {
 202              uint8 idata *cp;
 203              uint8 idata *cp1;
 204              uint8 temp, i;
 205              
 206              cp = OSTsakStackBotton[TaskID];
 207              cp1 = OSTsakStackBotton[TaskID + 1];
 208              temp = cp1 - cp;
 209              for (i = TaskID + 1; i <= OSRunningTaskID(); i++)
 210              {
 211                  OSTsakStackBotton[i] -= temp;
 212              }
 213              i = SP - (uint8)(cp1) + 1;
 214              do
 215              {
 216                  *cp++ = *cp1++;
 217              } while (--i != 0);
 218              SP = SP - temp;
 219          }
